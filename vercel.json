{
    "version": 2,
    "name": "romulomonte",
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "regions": ["gru1"],
    "cleanUrls": true,
    "trailingSlash": false
  }